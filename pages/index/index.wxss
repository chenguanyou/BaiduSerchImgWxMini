.page {
  height: 100%;
  background: #fc4234;
  z-index: 999999;
}

.container {
  display: flex;
  flex-direction: column;
  background-size: 100% 100%;
  width: 100%;
  background: #fc4234;
  /* padding-top: 80rpx; */
}

.indeximages {
  vertical-align: middle;
  width: 50px;
  height: 50px;
  padding-left: 150rpx;
}

.indeximages image {
  width: 50rpx;
  height: 50rpx;
  align-content: center;
  padding-top: 25rpx;
}

.search_class {
  display: flex;
  margin-top: 10px;
  flex-direction: row;
  padding-left: 10px;
  padding-right: 10px;
  margin-left: 20px;
  margin-right: 20px;
  align-items: center;
  /* margin-top: -400rpx; */
  border: 1px solid #fff;
  height: 100rpx;
}

.search_class input {
  z-index: 99999;
  color: #fff;
  font-size: 33rpx;
  padding-top: 10px;
  padding-bottom: 10px;
  width: 420rpx;
  position: absolute;
  left: 60rpx;
}

.input-placeholder {
  color: #fff;
  font-size: 33rpx;
  text-align: left;
}

.input_image {
  width: 50rpx;
  height: 50rpx;
  /* margin-left:45px; */
  margin-right: 30rpx;
  text-align: right;
  position: absolute;
  right: 168rpx;
}

.title_index {
  display: flex;
  flex-direction: row;
  padding-top: 350rpx;
  text-align: center;
  align-items: center;
  align-content: center;
  margin: 0 auto;
  color: #fff;
  font-weight: 200;
  font-size: 80rpx;
}

.input_button {
  font-family: "黑体";
  border-left: 1px solid #fff;
  font-size: 38rpx;
  font-weight: bold;
  color: #fff;
  text-align: right;
  padding-left: 30rpx;
  position: absolute;
  right: 60rpx;
  /* top:0; */
}

.title_index image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 10rpx;
}
